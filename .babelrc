{
    "env": {
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "modules": "commonjs",
                        "targets": {
                            "node": true
                        }
                    }
                ]
            ]
        },
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "modules": "false",
                        "targets": {
                            "node": true
                        }
                    }
                ]
            ],
            "plugins": [
                "@babel/plugin-proposal-optional-chaining",
                "@babel/plugin-syntax-bigint"
            ]
        }
    }
}